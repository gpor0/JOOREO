<configuration xmlns="http://www.jooq.org/xsd/jooq-codegen-3.16.0.xsd">
    <generator>
        <name>com.github.gpor0.jooreoGen.JooreoGen</name>
        <database>
            <name>org.jooq.meta.extensions.liquibase.LiquibaseDatabase</name>
            <properties>
                <property>
                    <key>scripts</key>
                    <value>database.xml</value>
                </property>
                <property>
                    <key>includeLiquibaseTables</key>
                    <value>false</value>
                </property>
            </properties>
        </database>
        <strategy>
            <!--matchers>
                <tables>
                    <table>
                        <recordClass>
                            <transform>PASCAL</transform>
                            <expression>Base_$0_Record</expression>
                        </recordClass>
                    </table>
                </tables>
            </matchers-->
            <name>com.github.gpor0.jooreoGen.JooreoGeneratorStrategy</name>
        </strategy>
        <target>
            <packageName>com.github.gpor0.jooreo.dao.records</packageName>
            <directory>target/generated-test-sources/jooq</directory>
        </target>
    </generator>
</configuration>
